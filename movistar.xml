            vars =re.findall(r"(?s)function\s+\w+\(\)\{\s*\w+\s*=\s*document.getElementById\('([^']+).*?\w+\s*=\s*document.getElementById\('([^']+).*?\w+\s*=\s*document.getElementById\('([^']+).*?\w+\s*=\s*document.getElementById\('([^']+)", html)
